<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/css/index/index.css" />
  </head>
  <body style="display: none;">
    <!-- 顶部 -->
    <%- include("./template/index/header.ejs") %>
    <!-- 中间区域 -->
    <%- include("./template/index/main.ejs") %>
    <!-- 底部 -->
    <%- include("./template/index/footer.ejs") %>
    <!-- seajs -->
    <script src="js/lib/seajs/sea.js"></script>
    <script>
      // Set configuration
      seajs.config({
        base: "/",
        alias: {
          jquery: "js/lib/jquery.min.js",
          //页面弹框插件
          layer: "layer.js",
          util: "js/util/util.js"
        },
        preload: ["jquery", "util"]
      });
      seajs.use(
        ["jquery", "layer", "js/util/config.js", "util", "js/index/index.js"],
        function($) {
          TOSEEINDEX.init();
        }
      );
    </script>
  </body>
</html>

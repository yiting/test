<div class="sec case">
    <div class="layout">
        <h1 id="experience">功能体验</h1>
        <div class="case-list">
            <!--<a class="btn" href="##" target="_blank" _stat_click_id="footer_console">本地上传</a>-->

            <div class="upload-panel">
                <form class="upload-form" enctype="multipart/form-data" method="post" name="fileinfo" id="uploadForm">
                    <a href="javascript:;" class="choose-file-btn">上传设计稿
                        <input type="file" name="content"/>
                    </a>
                    <!--<div class="upload-btn" id="upload-btn">上传文件</div>-->
                    <div class="file-info">
                        <span class="file-name"></span>
                        <span class="file-size"></span>
                    </div>
                </form>
            </div>
            <p class="upload-tips">注意：内测版，暂时只支持sketch文件格式</p>
        </div>
    </div>
</div>

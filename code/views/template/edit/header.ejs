<header id="app">
  <div class="header-left">
    <ul class="tab header-list">
      <li class="icon-logo"></li>
      <li class="header-item current">页面</li>
      <li class="header-item">img</li>
      <!-- <li class="header-item structure">结构</li> -->
    </ul>
  </div>
  <div class="header-center">
    <div class="zoom-widget">
      <button class="zoom-in"></button> <label class="zoom-text">100%</label>
      <button class="zoom-out"></button>
    </div>
    <!-- 获取当前设计稿图按钮 -->
    <div class="btn-operate-group">
      <div class="design-img-btn" data-show="0">比对设计稿</div>
      <!-- <div class="ai-rate-btn" data-show="0">显示命中率</div> -->
    </div>
    <div v-if="user.staffname!=''" class="user-info back-person-btn">
      <a href="#" class="nav-link">
        <span
          v-bind:style="{backgroundImage: 'url(http://dcloud.oa.com/Public/Avatar/'+user.staffname+'.png)'}"
          class="avatar"
        ></span
        ><span class="user-name" v-text="user.staffname"></span
      ></a>
    </div>
  </div>
  <div class="header-right">
    <div id="unit" class="unit-box" tabindex="0"></div>
  </div>
  <div class="qr">
    <i>
      <div class="qr-code"></div>
      <input class="current-url" type="text" value="" readonly />
    </i>
  </div>
</header>
